<module rename-to='aquapulse'>
    <inherits name='com.badlogic.gdx.backends.gdx_backends_gwt'/>

    <source path='core'/>
    <source path='html'/>

    <public path="resources"/>

    <entry-point class='com.soupcan.aquapulse.html.AquapulseHtml'/>
    <set-configuration-property name="gdx.assetpath"
                                value="${asset.input.dir},desktop/assets,../desktop/assets,../../../../desktop/assets/"/>
    <set-configuration-property name="gdx.assetoutputpath"
                                value="${asset.output.dir},aquapulse-0.1-SNAPSHOT,src/main/webapp,./"/>
</module>
